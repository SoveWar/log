<%= form_with(model: windows_event, local: true) do |form| %>
  <% if windows_event.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(windows_event.errors.count, "error") %> prohibited this windows_event from being saved:</h2>

      <ul>
      <% windows_event.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :event_id %>
    <%= form.number_field :event_id %>
  </div>

  <div class="field">
    <%= form.label :client_id %>
    <%= form.number_field :client_id %>
  </div>

